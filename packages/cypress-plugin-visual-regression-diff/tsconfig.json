{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@fixtures/*": ["__tests__/partials/*"],
      "@mocks/*": ["__tests__/mocks/*"]
    },
    "resolveJsonModule": true
  },
  "include": ["packages/*/src/*.ts", "vitest.config.ts"]
}
